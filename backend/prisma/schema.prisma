datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Staff {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String   
  email     String?
  name      String
  role_id   Int      @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}